<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import com.jfoenix.controls.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<fx:root id="AnchorPane" type="AnchorPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox spacing="35.0">
         <children>
            <JFXTextField fx:id="txtGoalName" styleClass="test" />
            <HBox alignment="TOP_RIGHT" spacing="20.0" VBox.vgrow="ALWAYS">
               <children>
                  <JFXButton fx:id="btnNew" onAction="#addNewGoal" styleClass="button-raised" text="Nieuw" />
                  <JFXButton fx:id="btnSave" onAction="#saveGoal" styleClass="button-raised" text="Opslaan" />
                  <JFXButton onAction="#deleteGoal" text="Verwijder" />
               </children>
            </HBox>
         </children>
         <padding>
            <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
         </padding>
      </VBox>
   </children>
</fx:root>
